{
  "name": "weather-view",
  "version": "0.1.0",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "format:check": "npx prettier --ignore-path .gitignore --check \"./**/*.{ts,tsx,json,md,html,scss}\"",
    "format:fix": "npx prettier --ignore-path .gitignore --write \"./**/*.{ts,tsx,json,md,html,scss}\"",
    "lint:check": "eslint . --ext ts,tsx",
    "types:check": "npx tsc -p tsconfig.json",
    "audit:check": "npm audit --production",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@craco/craco": "^6.4.0",
    "@react-leaflet/core": "1.0.2",
    "classnames": "^2.3.1",
    "leaflet": "^1.7.1",
    "lodash.debounce": "^4.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hotkeys-hook": "^3.4.4",
    "react-icons": "^4.3.1",
    "react-leaflet": "3.1.0",
    "react-useportal": "^1.0.14",
    "react-vis": "^1.11.7",
    "ts-open-weather-map": "^0.0.9"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/node-logger": "^6.3.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/leaflet": "^1.7.6",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^12.20.37",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@types/react-vis": "^1.11.10",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "autoprefixer": "^9.8.8",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest-mock-extended": "^2.0.4",
    "postcss": "^7.0.39",
    "prettier": "^2.4.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "react-scripts": "4.0.3",
    "storybook-addon-mock": "^1.7.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
    "typescript": "^4.4.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "testMatch": [
      "**/*.test.(ts|tsx)"
    ],
    "clearMocks": true
  }
}
